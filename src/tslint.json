{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "time",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "time",
            "kebab-case"
        ],
  
        "no-unused-expression": [true, "allow-fast-null-checks"],
        "comment-format": true
    }
  }
  